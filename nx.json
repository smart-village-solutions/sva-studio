{
  "installation": {
    "version": "22.3.3",
    "plugins": {
      "@nx/react": "22.3.3",
      "@nx/node": "22.3.3",
      "@nx/jest": "22.3.3",
      "@nx/eslint": "22.3.3"
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["format", "lint", "test", "build"],
        "parallel": 3
      }
    }
  },
  "targetDefaults": {
    "format": {
      "inputs": ["default"]
    },
    "lint": {
      "inputs": ["default"]
    },
    "test": {
      "inputs": ["default"],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "build": {
      "inputs": ["production"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "dependsOn": ["^build"]
    }
  }
}