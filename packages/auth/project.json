{
  "name": "auth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/auth/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc -p packages/auth/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('lint not configured for @sva/auth')\""
      }
    },
    "test:unit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/auth && vitest run --reporter=verbose"
      }
    }
  },
  "tags": ["scope:auth", "type:lib"]
}
