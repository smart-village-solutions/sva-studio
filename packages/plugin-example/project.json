{
  "name": "plugin-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugin-example/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc -p packages/plugin-example/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @sva/plugin-example exec tsc -p tsconfig.lib.json --noEmit"
      }
    },
    "test:unit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('unit tests not configured for @sva/plugin-example (coverage-exempt)')\""
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('coverage tests not configured for @sva/plugin-example (coverage-exempt)')\""
      }
    },
    "test:integration": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('integration tests not configured for @sva/plugin-example (coverage-exempt)')\""
      }
    }
  },
  "tags": ["scope:plugin", "type:lib"]
}
