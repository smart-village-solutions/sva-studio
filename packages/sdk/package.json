{
  "name": "@sva/sdk",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./logger/index.server": {
      "types": "./dist/logger/index.server.d.ts",
      "default": "./dist/logger/index.server.js"
    },
    "./middleware/request-context.server": {
      "types": "./dist/middleware/request-context.server.d.ts",
      "default": "./dist/middleware/request-context.server.js"
    },
    "./observability/context.server": {
      "types": "./dist/observability/context.server.d.ts",
      "default": "./dist/observability/context.server.js"
    }
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/winston-transport": "^0.21.0",
    "@sva/core": "workspace:*",
    "@sva/monitoring-client": "workspace:*",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
